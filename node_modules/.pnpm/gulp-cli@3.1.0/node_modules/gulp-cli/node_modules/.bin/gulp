#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules/gulp-cli/bin/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules/gulp-cli/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules/gulp-cli/bin/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules/gulp-cli/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/gulp-cli@3.1.0/node_modules:/Users/audioj/Workspaces/quoin/pdfjs_viewer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gulp.js" "$@"
else
  exec node  "$basedir/../../bin/gulp.js" "$@"
fi
